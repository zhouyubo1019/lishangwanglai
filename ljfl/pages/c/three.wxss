/* pages/book/book.wxss */
.book-container{
  width:100%;
  height:100vh;
  background:#efefef;
  overflow: hidden;
}
.bcz-box{
  width:700rpx;
  height:150rpx;
  background:#fff;
  margin:22rpx auto;
  border-radius: 16rpx;
  line-height: 150rpx;
  padding:0 30rpx;
  overflow: hidden;
  box-sizing: border-box; 
}
.bcz-box image{
  width:84rpx;
  height:84rpx;
  vertical-align: middle;
  margin-right:33rpx;
}
.item-title{
  font-size:34rpx;
  font-weight:bold;
  color:#000;
}
.item-navto{
  float: right;
  font-size:30rpx;
  color:#d9d9d9;
}